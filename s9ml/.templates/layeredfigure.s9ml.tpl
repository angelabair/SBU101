<?xml version='1.0' encoding='UTF-8'?>
<layeredfigure xmlns="http://www.standardnine.com/s9ml" designation="{{designation}}" enumeration="{{enumeration}}" >
    <title>{{title}}</title>
    <caption>Test Yourself figures allow users to hide the labels on a diagram and test their recall of the labels, one by one. This exhibit has two image layers: one image has labels, the other image has its labels removed and replaced with poptips.</caption>
    <cite/>
    <type>illustration</type>
    <layers>
        <layer>
            <img src="../../img/.templates/placeholder_labels.png"/>
        </layer>
        <layer>
            <img src="../../img/.templates/placeholder_art.png"/>
            <annotations type="assessment">
                <!--This is the first poptip-->
                <annotation>
                    <orientation>tailright</orientation>
                    <location x="195" y="228"/>
                    <snippet><text>Label poptip 1</text></snippet>
                </annotation>
                <!--This is the second poptip-->
                <annotation>
                    <orientation>tailup</orientation>
                    <location x="610" y="670"/>
                    <snippet><text>Label poptip 2</text></snippet>
                </annotation>
            </annotations>
        </layer>
    </layers>
</layeredfigure>
