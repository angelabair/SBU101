<?xml version='1.0' encoding='UTF-8'?>
<guidedtour xmlns="http://www.standardnine.com/s9ml" designation="{{designation}}" enumeration="{{enumeration}}" >
    <title>{{title}}</title>
    <img src="../../img/.templates/placeholder.png" />
    <caption>Guided Tours provide self-paced navigation across an image of a complex process, diagram, or concept. Additional information appears in a poptip at each step.</caption>
    <type>illustration</type>
    <steps>
