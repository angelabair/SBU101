            </annotation>
        </step>
        <!--Second step-->
        <step>
            <frame x="140" y="326" width="329" height="196"/>
            <annotation >
                <orientation>taildown</orientation>
                <location x="299" y="408"/>
                <snippet><text>...the second step...</text></snippet>
            </annotation>
        </step>
        <!--Third step-->
        <step>
            <frame x="581" y="471" width="248" height="240"/>
            <annotation >
                <orientation>tailup</orientation>
                <location x="701" y="611"/>
                <snippet><text>...and the third step.</text></snippet>
            </annotation>
        </step>
        </steps>
</guidedtour>
