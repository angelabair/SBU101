<?xml version='1.0' encoding='UTF-8'?>
<assessment xmlns="http://www.standardnine.com/s9ml" designation="{{designation}}" enumeration="{{enumeration}}">
    <title>{{title}}</title>
    <questions randomorder="false">  
<!-- Multiple Choice question type -->
        <multichoice randomorder="false">
            <question><i>This is an example of a standard Multiple Choice question.</i> This part of the question is termed the “stem.” After reading the stem, a user can choose to select an answer choice or reveal a hint.</question>  
            <hint>
                <text>Hints can help guide users in the right direction, without indicating the correct response. Users can get the help they need while still getting the most out of a question.</text>
            </hint> 
            <choices>
                <choice correct="false">
                    <text>Incorrect answer choice A <i>(Select this answer first to see feedback.)</i></text>
                    <feedback>
                        <text>This represents an incorrect response. Feedback can be given to help a user understand where they went wrong or to clear up misconceptions. Now try selecting answer choice D.</text>
                    </feedback>
                </choice>
                <choice correct="false">
                    <text>Incorrect answer choice B</text>
                    <feedback>
                        <text>This represents an incorrect response.</text>
                    </feedback>
                </choice>
                <choice correct="false">
                    <text>Incorrect answer choice C</text>
                    <feedback>
                        <text>This represents an incorrect response.</text>
                    </feedback>
                </choice>
                <choice correct="true">
                    <text>Correct answer choice D</text>
                    <feedback>
                        <text>This represents a correct response. Feedback can be provided to reinforce the understanding of the concept being tested. Users can attempt a question many times until they uncover the correct response.</text>
                    </feedback> 
                </choice>
            </choices>
        </multichoice>
<!-- END of Multiple Choice question type -->
<!-- Multiple Answer question type -->
        <multianswer randomorder="false">
            <question><i>This is an example of a Multiple Answer question.</i> Which of these answers are correct?</question>
            <hint>
                <text>A hint can go here.</text>
            </hint>
            <choices>
                <choice correct="true">
                    <text>Correct answer choice A</text>
                </choice>
                <choice correct="true">
                    <text>Correct answer choice B</text>
                </choice>
                <choice correct="true">
                    <text>Correct answer choice C</text>
                </choice>
                <choice correct="false">
                    <text>Incorrect answer choice D</text>

                </choice>
            </choices>
        </multianswer>
<!-- END of Multiple Answer question type -->
<!-- Ordered question type -->
        <orderedchoice>
            <question><i>This is an example of an Ordering question.</i> Place the following answers in the correct order.</question>
            <hint>
                <text>A hint can go here.</text>
            </hint>
            <choices>
                <choice>
                    <text>Step 1</text>
                </choice>
                <choice>
                    <text>Step 2</text>
                </choice>
                <choice>
                    <text>Step 3</text>
                </choice>
                <choice>
                    <text>Step 4</text>
                </choice>
                <choice>
                    <text>Step 5</text>
                </choice>
                <choice>
                    <text>Step 6</text>
                </choice>
            </choices>
        </orderedchoice>
<!-- END of Ordered question type -->
<!-- True/False question type -->
        <truefalse>
            <question><i>This is an example of a True or False question.</i> This statement is true.</question>
            <choices>
                <true correct="true">
                    <feedback>
                        <text>Feedback can appear here.</text>
                    </feedback>
                </true>
                <false correct="false">
                    <feedback>
                        <text>Feedback about the incorrect response can appear here.</text>
                    </feedback>
                </false>
            </choices>
        </truefalse>
<!-- END of True/False question type -->
<!-- Short Answer question type -->
        <shortanswer>
            <question><i>This is an example of a Short Answer question.</i> Write a short descriptive answer here.</question>
            <hint>
                <text>A hint can go here.</text>
            </hint>
            <feedback>
                <text>Here’s the correct response. Users can read this response after they submit and decide if their answer was correct.</text>
            </feedback>
        </shortanswer>
<!-- END of Short Answer question type -->
<!-- Matching question type -->
        <matching>
            <question><i>This is an example of a Matching question.</i> Match the answers with the labels.</question>
            <options reusable="true">
                <option>
                    <text>Label 0</text>
                </option>
                <option>
                    <text>Label 1</text>
                </option>
                <option>
                    <text>Label 2</text>
                </option>
            </options>
            <choices>
                <choice options="2">
                    <text>Matches with label 2</text>
                </choice>
                <choice options="0">
                    <text>Matches with label 0</text>
                </choice>
                <choice options="1">
                    <text>Matches with label 1</text>
                </choice>
                <choice options="1">
                    <text>Matches with label 1</text>
                </choice>
            </choices>
        </matching>
<!-- END of Matching question type -->
        </questions>
    <result/>
</assessment>