<?xml version='1.0' encoding='UTF-8'?>
<sliderfigure xmlns="http://www.standardnine.com/s9ml" designation="{{designation}}" enumeration="{{enumeration}}">
    <title>{{title}}</title>
    <!-- <caption> must be present, can be empty -->
    <caption>A Slideline lets the user transition through a sequence of images, using a slider control at the bottom of the frame. The images can appear on the slider at evenly spaced intervals, or at specific values.</caption>
    <type>illustration</type>
    <slider discrete="false">
        <start value="0">Start</start>
        <step value="0">
            <img src="../../img/.templates/placeholder_1.png" />
            <!-- <label> optional -->
            <label>1</label>
            <!-- <caption> optional -->
            <caption>Caption for first step</caption> 
        </step>
        <step value="1">
            <img src="../../img/.templates/placeholder_2.png" />
            <!-- <label> optional -->
            <label>2</label>
            <!-- <caption> optional -->
            <caption>Caption for second step</caption> 
        </step>
        <step value="2">
            <img src="../../img/.templates/placeholder_3.png" />
            <!-- <label> optional -->
            <label>3</label>
            <!-- <caption> optional -->
            <caption>Caption for third step</caption> 
        </step>
        <end value="2">End</end>
    </slider>
</sliderfigure>
