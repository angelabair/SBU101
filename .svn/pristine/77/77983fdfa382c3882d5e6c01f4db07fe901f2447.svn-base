// ## Equation Key
// Used for equations with numbers.

.equation-key {
	text-align: center;
	h4 {
		font-size: 1.2em;
		font-weight: 400;
		text-align: center;
	}
	.key-number {
		text-align: center;
		padding: 0;
		font-family: $font-chronicle;
		font-size: .8em;
		font-weight: 400;
		line-height: 1em;
		color: #000;
	}
	img.equation {
		margin-top: 1em;
		margin-bottom: 1em;
	}
}
