<?xml version='1.0' encoding='UTF-8'?>
<hotspotfigure xmlns="http://www.standardnine.com/s9ml" designation="{{designation}}" enumeration="{{enumeration}}">
    <title>{{title}}</title>
  	<!-- <caption> must be present, can be empty -->
  	<caption>Hotspot figures let users zoom into an image to provide a closer look or more detail.</caption>
    <imagefigure>
        <img src="../../img/.templates/placeholder_hotspot.png" />
        <hotspots>
            <hotspot>
                <frame x="643" y="158" width="281" height="218"/>
                <imagefigure>
                    <img src="../../img/.templates/placeholder.png" />
                    <!-- <caption> optional -->
                    <caption>A detail view</caption>
                </imagefigure>
            </hotspot>
        </hotspots>
    </imagefigure>
</hotspotfigure>
